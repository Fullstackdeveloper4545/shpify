{%- style -%}
.about-us {
  max-width: none;
  margin: 0;
  padding: 0 0 64px;
  display: grid;
  gap: 42px;
}
.about-us__hero {
  position: relative;
  border-radius: 0;
  overflow: hidden;
  min-height: 540px;
  width: 100%;
  background: url({{ '375575f2055d0844c1846f5327242710f8108f75.jpg' | asset_url }}) center / cover no-repeat;
}
.about-us__hero img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  display: block;
}
.about-us__hero-overlay {
  position: absolute;
  inset: 0;
  background: linear-gradient(180deg, rgba(0,0,0,0.35) 0%, rgba(0,0,0,0.55) 100%);
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  padding: 32px;
}
.about-us__hero h1 {
  color: #fff;
  font-family: 'Poppins', sans-serif;
  font-size: 42px;
  font-weight: 500;
  margin: 0 0 12px;
  line-height: 140%;
  letter-spacing: 0;
}
.about-us__hero p {
  color: #fff;
  font-family: 'Poppins', sans-serif;
  font-size: 20px;
  font-weight: 500;
  letter-spacing: 0;
  line-height: 135%;
  margin: 0;
}
.about-us__grid {
  display: grid;
  grid-template-columns: 1fr 2fr;
  gap: 148px;
  align-items: start;
  max-width: 1200px;
  margin: 66px auto;
  padding: 0 16px;
}
.about-us__grid--mission {
  align-items: center;
  text-align: center;
}
.about-us__grid--services { align-items: flex-start; }
.about-us__grid--mission .about-us__title,
.about-us__grid--mission .about-us__text {
  justify-self: center;
}
.about-us__grid--mission .about-us__title { text-align: center; }
.about-us__grid--mission .about-us__text { text-align: center; }
.about-us__title {
  font-family: 'Poppins', sans-serif;
  font-size: 42px;
  font-weight: 500;
  font-style: normal;
  line-height: 46px;
  letter-spacing: 0;
  margin: 0 0 12px;
}
.about-us__text {
  font-family: 'Poppins', sans-serif;
  font-size: 18px;
  font-weight: 400;
  font-style: normal;
  letter-spacing: 0;
  line-height: 1.7;
  color: #2f2f2f;
}
.about-us__list ul {
  margin: 0;
  padding-left: 20px;
  display: grid;
  gap: 12px;
}
.about-us__list li { margin: 0; }
.about-us__image img {
  width: 100%;
  border-radius: 12px;
  object-fit: cover;
  display: block;
}
.about-us__card {
  background: #000;
  border-radius: 0;
  padding: 40px 16px;
  line-height: 1.6;
  font-size: 16px;
  color: #fff;
  max-width: none;
  min-height: 561px;
  margin: 0;
}
.about-us__usps {
  display: grid;
  grid-template-columns: 1.4fr 1fr;
  gap: 40px;
  align-items: stretch;
  max-width: 1200px;
  margin: 32px auto;
  padding: 0 16px;
}
.about-us__usps-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));
  gap: 28px;
  align-items: stretch;
}
.about-us__usp-card {
  border: 1px solid #e6e6e6;
  border-radius: 16px;
  padding: 32px 20px;
  text-align: center;
  background: #fff;
  display: grid;
  gap: 14px;
  justify-items: center;
}
.about-us__usp-card h4 {
  margin: 0;
  font-family: 'Poppins', sans-serif;
  font-weight: 400;
  font-size: 20px;
  line-height: 1;
  letter-spacing: 0;
  color: #0f0f0f;
}
.about-us__usp-card svg {
  width: 72px;
  height: auto;
}
.about-us__usp-copy {
  background: #ff6701;
  color: #fff;
  border-radius: 18px;
  padding: 32px 28px;
  font-family: 'Poppins', sans-serif;
  font-size: 18px;
  font-weight: 400;
  line-height: 1.7;
  letter-spacing: 0;
  display: grid;
  gap: 14px;
}
.about-us__usp-copy strong { font-weight: 600; }
@media (max-width: 749px) {
  .about-us {
    padding: 0 0 48px;
    gap: 32px;
  }
  .about-us__hero {
    min-height: 360px;
  }
  .about-us__hero h1 { font-size: 28px; }
  .about-us__hero p { font-size: 16px; }
  .about-us__title { font-size: 22px; }
  .about-us__grid {
    grid-template-columns: 1fr;
    gap: 20px;
  }
  .about-us__card {
    padding: 28px 18px;
    min-height: auto;
    text-align: left;
    margin: 0 0 12px;
  }
  .about-us__card h3,
  .about-us__card div {
    text-align: left !important;
    margin-left: 0 !important;
    margin-right: 0 !important;
  }
  .about-us__usps {
    grid-template-columns: 1fr;
    gap: 24px;
    padding: 0 12px;
  }
  .about-us__usps-grid {
    grid-template-columns: 1fr 1fr;
    gap: 18px;
  }
  .about-us__usp-card {
    padding: 24px 14px;
  }
}
{%- endstyle -%}

<section class="about-us" data-section-id="{{ section.id }}">
  <div class="about-us__hero">
    {% if section.settings.hero_image != blank %}
      {{ section.settings.hero_image | image_url: width: 2000 | image_tag: alt: section.settings.hero_heading, loading: 'lazy', style: 'width:100%;height:100%;object-fit:cover;display:block;' }}
    {% endif %}
    <div class="about-us__hero-overlay">
      <div>
        <h1>{{ section.settings.hero_heading | newline_to_br }}</h1>
        <p>{{ section.settings.hero_subheading | newline_to_br }}</p>
      </div>
    </div>
  </div>

  <div class="about-us__grid">
    <div>
      <h2 class="about-us__title">{{ section.settings.history_heading }}</h2>
    </div>
    <div class="about-us__text">
      {{ section.settings.history_text | newline_to_br }}
    </div>
  </div>

  <div class="about-us__grid about-us__grid--mission">
    <div class="about-us__image">
      {% assign fallback_mission_image = '093c3cb500d0a4fb65114bfc5ec76f2133fc8146.jpg' | asset_url %}
      {% if section.settings.mission_image != blank %}
        {{ section.settings.mission_image | image_url: width: 1200 | image_tag: alt: section.settings.mission_heading, loading: 'lazy' }}
      {% else %}
        <img src="{{ fallback_mission_image }}" alt="{{ section.settings.mission_heading | escape }}" loading="lazy" style="width:100%;max-width:602px;height:auto;object-fit:cover;border-radius:10px;">
      {% endif %}
    </div>
    <div>
      <h2 class="about-us__title"style="font-size:42px;font-weight:500;line-height:100%;text-align: left;letter-spacing:0;">{{ section.settings.mission_heading }}</h2>
      <div class="about-us__text" style="font-size:18px;font-weight:400;line-height:1.7;letter-spacing:0;">{{ section.settings.mission_text | newline_to_br }}</div>
    </div>
  </div>

  <div class="about-us__card" style="text-align:left; display:flex; flex-direction:column; justify-content:center; padding:40px 24px;">
    <h3 style="margin-top:0;margin-bottom:12px;font-family:'Poppins',sans-serif;font-weight:500;font-size:42px;line-height:48px;letter-spacing:0;color:#fff;text-align:left;margin-left:0;">
      {{ section.settings.founders_heading | newline_to_br }}
    </h3>
    <div style="font-family:'Poppins',sans-serif;font-weight:300;font-size:18px;line-height:1.7;letter-spacing:0;color:#fff;text-align:left;margin-left:0;">
      {{ section.settings.founders_text | newline_to_br }}
    </div>
  </div>

  <div class="about-us__grid about-us__grid--services">
    <div>
      <h2 class="about-us__title" style="font-size:42px;font-weight:500;line-height:100%;text-align:left;letter-spacing:0;">
        {{ section.settings.services_heading }}
      </h2>
    </div>
    <div class="about-us__text about-us__list" style="font-size:18px;font-weight:400;line-height:1.7;letter-spacing:0;">
      {{ section.settings.services_text }}
    </div>
  </div>

  <div class="about-us__usps">
